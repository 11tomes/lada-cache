{
    "name": "spiritix/lada-cache",
    "type": "library",
    "description": "An automated and scalable database caching layer for Laravel 5+",
    "license": "MIT",
    "keywords": ["php","laravel","cache","caching","redis"],
    "authors": [
        {
            "name": "Matthias Isler",
            "email": "mi@matthias-isler.ch"
        }
    ],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "illuminate/database": "~5.1",
        "phpunit/phpunit": "5.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Spiritix\\LadaCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiritix\\LadaCache\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}